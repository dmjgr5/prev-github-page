    <span class="k">return</span> <span class="n">DateFormat</span><span class="p">(</span><span class="s">"h:mm a"</span><span class="p">).</span><span class="nb">format</span><span class="p">(</span><span class="n">now</span><span class="p">);</span>
  <span class="p">}</span>

  <span class="o">@</span><span class="n">override</span>
