<h4 id="navigator-ÌéòÏù¥ÏßÄ-Ïù¥Îèô">Navigator ÌéòÏù¥ÏßÄ Ïù¥Îèô</h4>

<p><img src="/assets/images/flutterex4.png" alt="example" /></p>

<div class="language-python highlighter-rouge"><div class="highlight"><pre class="highlight"><code><span class="k">import</span> <span class="s">'package:flutter/material.dart'</span><span class="p">;</span>
<span class="k">import</span> <span class="s">'package:fluttertoast/fluttertoast.dart'</span><span class="p">;</span>

<span class="n">void</span> <span class="n">main</span><span class="p">()</span> <span class="o">=&gt;</span> <span class="n">runApp</span><span class="p">(</span><span class="n">MyApp</span><span class="p">());</span>

<span class="k">class</span> <span class="nc">MyApp</span> <span class="n">extends</span> <span class="n">StatelessWidget</span> <span class="p">{</span>
  <span class="n">const</span> <span class="n">MyApp</span><span class="p">({</span><span class="n">Key</span><span class="err">?</span> <span class="n">key</span><span class="p">})</span> <span class="p">:</span> <span class="nb">super</span><span class="p">(</span><span class="n">key</span><span class="p">:</span> <span class="n">key</span><span class="p">);</span>

  <span class="o">@</span><span class="n">override</span>
  <span class="n">Widget</span> <span class="n">build</span><span class="p">(</span><span class="n">BuildContext</span> <span class="n">context</span><span class="p">)</span> <span class="p">{</span>
    <span class="k">return</span> <span class="n">MaterialApp</span><span class="p">(</span>
      <span class="n">title</span><span class="p">:</span> <span class="s">'SnackBar'</span><span class="p">,</span>
      <span class="n">theme</span><span class="p">:</span> <span class="n">ThemeData</span><span class="p">(</span><span class="n">primarySwatch</span><span class="p">:</span> <span class="n">Colors</span><span class="p">.</span><span class="n">blue</span><span class="p">),</span>
      <span class="n">home</span><span class="p">:</span> <span class="n">FirstPage</span><span class="p">(),</span>
    <span class="p">);</span>
  <span class="p">}</span>
<span class="p">}</span>

<span class="k">class</span> <span class="nc">FirstPage</span> <span class="n">extends</span> <span class="n">StatelessWidget</span> <span class="p">{</span>
  <span class="n">const</span> <span class="n">FirstPage</span><span class="p">({</span><span class="n">Key</span><span class="err">?</span> <span class="n">key</span><span class="p">})</span> <span class="p">:</span> <span class="nb">super</span><span class="p">(</span><span class="n">key</span><span class="p">:</span> <span class="n">key</span><span class="p">);</span>

  <span class="o">@</span><span class="n">override</span>
  <span class="n">Widget</span> <span class="n">build</span><span class="p">(</span><span class="n">BuildContext</span> <span class="n">context2</span><span class="p">)</span> <span class="p">{</span>
    <span class="k">return</span> <span class="n">Scaffold</span><span class="p">(</span>
      <span class="n">appBar</span><span class="p">:</span> <span class="n">AppBar</span><span class="p">(</span>
        <span class="n">title</span><span class="p">:</span> <span class="n">Text</span><span class="p">(</span><span class="s">'frist page'</span><span class="p">),</span>
      <span class="p">),</span>
      <span class="n">body</span><span class="p">:</span> <span class="n">Center</span><span class="p">(</span>
        <span class="n">child</span><span class="p">:</span> <span class="n">RaisedButton</span><span class="p">(</span>
          <span class="n">child</span><span class="p">:</span> <span class="n">Text</span><span class="p">(</span><span class="s">'Go to the second page'</span><span class="p">),</span>
          <span class="n">onPressed</span><span class="p">:</span> <span class="p">()</span> <span class="p">{</span>
            <span class="n">Navigator</span><span class="p">.</span><span class="n">push</span><span class="p">(</span>
                <span class="n">context2</span><span class="p">,</span> <span class="n">MaterialPageRoute</span><span class="p">(</span><span class="n">builder</span><span class="p">:</span> <span class="p">(</span><span class="n">_</span><span class="p">)</span> <span class="o">=&gt;</span> <span class="n">SecondPage</span><span class="p">()));</span>
          <span class="p">},</span>
        <span class="p">),</span>
      <span class="p">),</span>
    <span class="p">);</span>
  <span class="p">}</span>
<span class="p">}</span>

<span class="k">class</span> <span class="nc">SecondPage</span> <span class="n">extends</span> <span class="n">StatelessWidget</span> <span class="p">{</span>
  <span class="n">const</span> <span class="n">SecondPage</span><span class="p">({</span><span class="n">Key</span><span class="err">?</span> <span class="n">key</span><span class="p">})</span> <span class="p">:</span> <span class="nb">super</span><span class="p">(</span><span class="n">key</span><span class="p">:</span> <span class="n">key</span><span class="p">);</span>

  <span class="o">@</span><span class="n">override</span>
  <span class="n">Widget</span> <span class="n">build</span><span class="p">(</span><span class="n">BuildContext</span> <span class="n">ctx</span><span class="p">)</span> <span class="p">{</span>
    <span class="k">return</span> <span class="n">Scaffold</span><span class="p">(</span>
      <span class="n">appBar</span><span class="p">:</span> <span class="n">AppBar</span><span class="p">(</span>
        <span class="n">title</span><span class="p">:</span> <span class="n">Text</span><span class="p">(</span><span class="s">'second page'</span><span class="p">),</span>
