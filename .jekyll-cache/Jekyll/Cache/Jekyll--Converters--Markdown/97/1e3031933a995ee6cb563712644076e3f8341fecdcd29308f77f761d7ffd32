        <span class="n">backgroundColor</span><span class="p">:</span> <span class="n">Colors</span><span class="p">.</span><span class="n">redAccent</span><span class="p">,</span>
        <span class="n">centerTitle</span><span class="p">:</span> <span class="n">true</span><span class="p">,</span>
        <span class="n">leading</span><span class="p">:</span> <span class="n">IconButton</span><span class="p">(</span><span class="n">icon</span><span class="p">:</span> <span class="n">Icon</span><span class="p">(</span><span class="n">Icons</span><span class="p">.</span><span class="n">menu</span><span class="p">),</span> <span class="n">onPressed</span><span class="p">:</span> <span class="p">()</span> <span class="p">{}),</span>
        <span class="n">actions</span><span class="p">:</span> <span class="o">&lt;</span><span class="n">Widget</span><span class="o">&gt;</span><span class="p">[</span>
          <span class="n">IconButton</span><span class="p">(</span><span class="n">icon</span><span class="p">:</span> <span class="n">Icon</span><span class="p">(</span><span class="n">Icons</span><span class="p">.</span><span class="n">search</span><span class="p">),</span> <span class="n">onPressed</span><span class="p">:</span> <span class="p">()</span> <span class="p">{})</span>
        <span class="p">],</span>
      <span class="p">),</span>
      <span class="n">body</span><span class="p">:</span> <span class="n">Builder</span><span class="p">(</span>
