    <span class="p">}</span>
  <span class="p">}</span>


  <span class="n">Future</span><span class="o">&lt;</span><span class="n">dynamic</span><span class="o">&gt;</span> <span class="n">getAirData</span><span class="p">()</span> <span class="k">async</span> <span class="p">{</span>
