      <span class="n">appBar</span><span class="p">:</span> <span class="n">AppBar</span><span class="p">(</span>
        <span class="n">title</span><span class="p">:</span> <span class="n">Text</span><span class="p">(</span><span class="s">'BBANTO'</span><span class="p">),</span>
        <span class="n">backgroundColor</span><span class="p">:</span> <span class="n">Colors</span><span class="p">.</span><span class="n">amber</span><span class="p">[</span><span class="mi">700</span><span class="p">],</span>
        <span class="n">centerTitle</span><span class="p">:</span> <span class="n">true</span><span class="p">,</span>
        <span class="n">elevation</span><span class="p">:</span> <span class="mf">0.0</span><span class="p">,</span>
      <span class="p">),</span>
