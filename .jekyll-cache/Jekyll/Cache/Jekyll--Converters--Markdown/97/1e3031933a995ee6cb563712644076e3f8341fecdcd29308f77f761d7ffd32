          <span class="n">onPressed</span><span class="p">:</span> <span class="p">()</span> <span class="p">{</span>
            <span class="n">Navigator</span><span class="p">.</span><span class="n">pop</span><span class="p">(</span><span class="n">ctx</span><span class="p">);</span>
          <span class="p">},</span>
        <span class="p">),</span>
      <span class="p">),</span>
    <span class="p">);</span>
  <span class="p">}</span>
