  <span class="n">const</span> <span class="n">Loading</span><span class="p">({</span><span class="n">Key</span><span class="err">?</span> <span class="n">key</span><span class="p">})</span> <span class="p">:</span> <span class="nb">super</span><span class="p">(</span><span class="n">key</span><span class="p">:</span> <span class="n">key</span><span class="p">);</span>

  <span class="o">@</span><span class="n">override</span>
  <span class="n">_LoadingState</span> <span class="n">createState</span><span class="p">()</span> <span class="o">=&gt;</span> <span class="n">_LoadingState</span><span class="p">();</span>
<span class="p">}</span>

<span class="k">class</span> <span class="nc">_LoadingState</span> <span class="n">extends</span> <span class="n">State</span><span class="o">&lt;</span><span class="n">Loading</span><span class="o">&gt;</span> <span class="p">{</span>
  <span class="n">late</span> <span class="n">double</span> <span class="n">latitude3</span><span class="p">;</span>
  <span class="n">late</span> <span class="n">double</span> <span class="n">longitude3</span><span class="p">;</span>

  <span class="o">@</span><span class="n">override</span>
  <span class="n">void</span> <span class="n">initState</span><span class="p">()</span> <span class="p">{</span>
    <span class="o">//</span> <span class="n">TODO</span><span class="p">:</span> <span class="n">implement</span> <span class="n">initState</span>
    <span class="nb">super</span><span class="p">.</span><span class="n">initState</span><span class="p">();</span>
    <span class="n">getLocation</span><span class="p">();</span>
  <span class="p">}</span>

  <span class="n">void</span> <span class="n">getLocation</span><span class="p">()</span> <span class="k">async</span> <span class="p">{</span>
    <span class="n">MyLocation</span> <span class="n">myLocation</span> <span class="o">=</span> <span class="n">MyLocation</span><span class="p">();</span>
    <span class="k">await</span> <span class="n">myLocation</span><span class="p">.</span><span class="n">getMyCurrentLocation</span><span class="p">();</span>
    <span class="n">latitude3</span> <span class="o">=</span> <span class="n">myLocation</span><span class="p">.</span><span class="n">latitude2</span><span class="p">;</span>
    <span class="n">longitude3</span> <span class="o">=</span> <span class="n">myLocation</span><span class="p">.</span><span class="n">longitude2</span><span class="p">;</span>
    <span class="k">print</span><span class="p">(</span><span class="n">latitude3</span><span class="p">);</span>
    <span class="k">print</span><span class="p">(</span><span class="n">longitude3</span><span class="p">);</span>

    <span class="n">Network</span> <span class="n">network</span> <span class="o">=</span> <span class="n">new</span> <span class="n">Network</span><span class="p">(</span>
        <span class="s">'https://api.openweathermap.org/data/2.5/'</span>
            <span class="s">'weather?lat=$latitude3&amp;lon='</span>
            <span class="s">'$longitude3&amp;appid=$apikey&amp;units=metric'</span><span class="p">,</span>
        <span class="s">'https://api.openweathermap.org/data/2.5/'</span>
            <span class="s">'air_pollution?lat=$latitude3&amp;lon='</span>
            <span class="s">'$longitude3&amp;appid=$apikey'</span><span class="p">);</span>

    <span class="n">var</span> <span class="n">weatherData</span> <span class="o">=</span> <span class="k">await</span> <span class="n">network</span><span class="p">.</span><span class="n">getJsonData</span><span class="p">();</span>
    <span class="k">print</span><span class="p">(</span><span class="n">weatherData</span><span class="p">);</span>

    <span class="n">var</span> <span class="n">airData</span> <span class="o">=</span> <span class="k">await</span> <span class="n">network</span><span class="p">.</span><span class="n">getAirData</span><span class="p">();</span>
    <span class="k">print</span><span class="p">(</span><span class="n">airData</span><span class="p">);</span>

    <span class="n">Navigator</span><span class="p">.</span><span class="n">push</span><span class="p">(</span><span class="n">context</span><span class="p">,</span> <span class="n">MaterialPageRoute</span><span class="p">(</span><span class="n">builder</span><span class="p">:</span> <span class="p">(</span><span class="n">context</span><span class="p">)</span> <span class="p">{</span>
      <span class="k">return</span> <span class="n">WeatherScreen</span><span class="p">(</span>
        <span class="n">parseWeatherData</span><span class="p">:</span> <span class="n">weatherData</span><span class="p">,</span>
        <span class="n">parseAirPollution</span><span class="p">:</span> <span class="n">airData</span><span class="p">,</span>
      <span class="p">);</span>
    <span class="p">}));</span>
  <span class="p">}</span>

  <span class="o">//</span> <span class="n">void</span> <span class="n">fetchData</span><span class="p">()</span> <span class="k">async</span><span class="p">{</span>
  <span class="o">//</span>
  <span class="o">//</span>     <span class="n">var</span> <span class="n">myJson</span> <span class="o">=</span> <span class="n">pasingData</span><span class="p">[</span><span class="s">'weather'</span><span class="p">][</span><span class="mi">0</span><span class="p">][</span><span class="s">'description'</span><span class="p">];</span>
  <span class="o">//</span>     <span class="k">print</span><span class="p">(</span><span class="n">myJson</span><span class="p">);</span>
  <span class="o">//</span>     <span class="n">var</span> <span class="n">windSpeed</span> <span class="o">=</span> <span class="n">pasingData</span><span class="p">[</span><span class="s">'wind'</span><span class="p">][</span><span class="s">'speed'</span><span class="p">];</span>
  <span class="o">//</span>     <span class="n">var</span> <span class="n">sysId</span> <span class="o">=</span> <span class="n">pasingData</span><span class="p">[</span><span class="s">'id'</span><span class="p">];</span>
  <span class="o">//</span>     <span class="k">print</span><span class="p">(</span><span class="n">windSpeed</span><span class="p">);</span>
  <span class="o">//</span>     <span class="k">print</span><span class="p">(</span><span class="n">sysId</span><span class="p">);</span>
  <span class="o">//</span>   <span class="p">}</span> <span class="k">else</span> <span class="p">{</span>
  <span class="o">//</span>     <span class="k">print</span><span class="p">(</span><span class="n">response</span><span class="p">.</span><span class="n">statusCode</span><span class="p">);</span>
  <span class="o">//</span>   <span class="p">}</span>
  <span class="o">//</span> <span class="p">}</span>

  <span class="o">@</span><span class="n">override</span>
  <span class="n">Widget</span> <span class="n">build</span><span class="p">(</span><span class="n">BuildContext</span> <span class="n">context</span><span class="p">)</span> <span class="p">{</span>
    <span class="k">return</span> <span class="n">Scaffold</span><span class="p">(</span>
      <span class="n">backgroundColor</span><span class="p">:</span> <span class="n">Colors</span><span class="p">.</span><span class="n">amber</span><span class="p">,</span>
      <span class="n">body</span><span class="p">:</span> <span class="n">Center</span><span class="p">(</span>
        <span class="n">child</span><span class="p">:</span> <span class="n">SpinKitDoubleBounce</span><span class="p">(</span>
          <span class="n">color</span><span class="p">:</span> <span class="n">Colors</span><span class="p">.</span><span class="n">white</span><span class="p">,</span>
          <span class="n">size</span><span class="p">:</span> <span class="mf">80.0</span><span class="p">,</span>
        <span class="p">),</span>
      <span class="p">),</span>
    <span class="p">);</span>
  <span class="p">}</span>
<span class="p">}</span>

</code></pre></div></div>

<p><code class="language-plaintext highlighter-rouge">weather_screen.dart</code></p>

